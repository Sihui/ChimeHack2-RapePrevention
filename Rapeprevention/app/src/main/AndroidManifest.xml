<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hackmate.rapeprevention" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/logo"
        android:allowBackup="true"
        android:label="FriendMate"
        android:theme="@style/AppTheme"
        android:name=".MyApplication">>
        <activity
            android:name=".StartupActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReactionActivity"
            android:label="@string/title_activity_reaction" >
        </activity>
        <activity
            android:name=".AddContactActivity"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name=".TextMessageActivity"
            android:label="@string/title_activity_text_message" >
        </activity>
        <activity
            android:name=".TimerActivity"
            android:label="@string/title_activity_timer" >
        </activity>
        <activity
            android:name=".IntroActivity"
            android:label="intro_activity" >
        </activity>
    </application>
</manifest>
